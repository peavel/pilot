{
    "name": "peavel/pilot",
    "description": "A Simple Laravel Admin Package",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/peavel/pilot/issues",
        "source": "https://github.com/peavel/pilot"
    },
    "authors": [
        {
            "name": "Abderrahmene Merzoug",
            "email": "obaydmerz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/support": "~6.0|~7.0|~8.0",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.4",
        "laravel/ui": ">=1.0",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.1|~2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpcov": ">=6.0",
        "phpunit/phpunit": ">=8.0",
        "laravel/framework": "~6.0|~7.0|~8.0",
        "orchestra/testbench": ">=4.0",
        "laravel/browser-kit-testing": ">=5.0.0",
        "orchestra/testbench-browser-kit": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "PEAVEL\\Pilot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PEAVEL\\Pilot\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "PEAVEL\\Pilot\\PilotServiceProvider",
                "PEAVEL\\Pilot\\Providers\\PilotDummyServiceProvider"
            ]
        }
    },
    "suggest": {
        "laravel/legacy-factories": "Required to run Pilot tests with Laravel 8.x"
    }
}
